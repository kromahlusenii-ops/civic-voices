{
  "functions": {
    "app/api/report/start/stream/route.ts": {
      "maxDuration": 300
    },
    "app/api/report/start/route.ts": {
      "maxDuration": 300
    },
    "app/api/report/[id]/chat/route.ts": {
      "maxDuration": 120
    },
    "app/api/report/[id]/insights/route.ts": {
      "maxDuration": 180
    },
    "app/api/report/[id]/pdf/route.ts": {
      "maxDuration": 120
    },
    "app/api/report/[id]/pdf/template/route.ts": {
      "maxDuration": 60
    },
    "app/api/cron/alerts/route.ts": {
      "maxDuration": 120
    }
  },
  "crons": [
    {
      "path": "/api/cron/alerts",
      "schedule": "*/15 * * * *"
    },
    {
      "path": "/api/cron/data-cleanup",
      "schedule": "0 3 * * *"
    }
  ]
}
